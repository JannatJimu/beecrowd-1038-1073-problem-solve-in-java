import java.util.Scanner;
public class Main {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        Scanner in =new Scanner (System.in);
        int N=in.nextInt();
        for(int i=1;i<=10;i++)
        {
            System.out.println(i+" x "+N+" = "+i*N);
        }
    }
    
}
